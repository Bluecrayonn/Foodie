<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dummy">
	<select id="email" resultType="java.util.HashMap">
		select * from table1 where email = #{email} 
		 
	</select>
	<select id="login" resultType="java.util.HashMap">
		select email, password from table1 where email = #{email} 
		 
	</select>
</mapper>